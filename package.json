{
  "name": "Ares",
  "version": "0.0.1",
  "description": "",
  "author": "hau-tran",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:unit:cov": "jest --coverage",
    "test:unit:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "./scripts/run-e2e-tests.sh",
    "db:migrate:new": "dbmate new",
    "db:migrate:up:local": "dbmate --env-file .env --no-dump-schema up",
    "db:migrate:down:local": "dbmate --env-file .env --no-dump-schema down",
    "db:seed:local": "dbmate --env-file .env --no-dump-schema -d ./db/seeds migrate",
    "docker:down": "docker compose -f docker-compose.yml down -v",
    "docker:up": "docker compose -f docker-compose.yml up -d --build",
    "docker:up:local": "docker compose -f docker-compose.yml -f docker-compose.local.yml up -d --build",
    "docker:down:local": "docker compose -f docker-compose.yml -f docker-compose.local.yml down -v",
    "docker:up:test": "docker compose -f docker-compose.test.yml up --build -d",
    "docker:down:test": "docker compose -f docker-compose.test.yml down",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.806.0",
    "@nest-lab/throttler-storage-redis": "^1.1.0",
    "@nestjs-cls/transactional": "^2.7.0",
    "@nestjs-cls/transactional-adapter-pg-promise": "^1.3.19",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^11.1.3",
    "@nestjs/throttler": "^6.4.0",
    "@types/bcrypt": "^5.0.2",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "date-fns": "^4.1.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "ms": "^2.1.3",
    "multer": "1.4.5-lts.2",
    "multer-s3": "^3.0.1",
    "nestjs-cls": "^5.4.3",
    "nodemailer": "^6.10.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg-promise": "^11.13.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "stripe": "^18.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.2",
    "@types/ms": "^2.1.0",
    "@types/multer": "^1.4.12",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "cross-env": "^7.0.3",
    "dbmate": "^2.26.0",
    "dotenv": "^16.5.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.1.7",
    "jest": "^29.5.0",
    "lint-staged": "^15.5.1",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
